<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hospital.model.medicine.dao.MedicineDao">

    <update id="updateCount">
        update medicine
        set
        count = count + #{count},
        update_time = #{now}
        where id = #{id}
    </update>

</mapper>
